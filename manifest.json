{
    "short_name": "Skip",
    "manifest_version": 2,
    "name": "Skip Chegg questions in one-click",
    "description": "One-click skipping extension for Chegg QnA",
    "version": "2.4",
    "author": "Shubham Singh",
	"browser_action": {
        "default_icon": "images/chegg-logo.png",
        "default_title": "Skip question",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab"]
}
